        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@drawable/btn_reg_selector</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="Button_brand" parent="Widget.Material3.Button.OutlinedButton">
        <item name="backgroundTint">@color/light_grey</item>
        <item name="android:textAppearance">@style/Title2</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.Material3.Corner.Small.</item>
    </style>

    <style name="login_btn" parent="Widget.MaterialComponents.Button">
        <item name="android:radius">@dimen/btn_corner_radius</item>
        <item name="android:backgroundTint">@color/light_pink</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
    </style>


    <style name="ShapeAppearance.Material3.Corner.Small." parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/btn_corner_radius</item>
    </style>

</resources>