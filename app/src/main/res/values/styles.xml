<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavigationThemeCustom">
        <item name="labelVisibilityMode">labeled</item>
        <item name="enforceTextAppearance">true</item>
        <item name="enforceMaterialTheme">true</item>
        <item name="itemIconTint">@drawable/bot_nav_selector_color</item>
        <item name="itemTextColor">@drawable/bot_nav_selector_color</item>

        <item name="android:paddingTop">@dimen/bottom_navigation_icon_margin</item>
        <item name="android:paddingBottom">@dimen/bottom_navigation_text_margin</item>
    </style>


    <style name="LargeTitle1" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textSize">@dimen/txt_title_16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/sf</item>
    </style>


